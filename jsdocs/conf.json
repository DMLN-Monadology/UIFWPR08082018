{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": [ "." ],
    "exclude": [ "./node_modules","./src/common/bower_components","./src/components/foundation/base/bower_components","./www" ]
  },
  "plugins": [

  ],
  "templates": {
    "systemName"            : "HS Core Tools<a href=\"../ngdocs/index.html\" style=\"position:relative;top:15px\">ng-docs</a>",
    //"footer"                : "This is the footer",
    "copyright"             :  "Copyright message",
    "includeDate"           : "true",
    "navType"               : "vertical",
    "theme"                 : "cerulean",
    "linenums"              : "false",
    "collapseSymbols"       : "true",
    "inverseNav"            : "true",
    "outputSourceFiles"     : "true" ,
    "dateFormat"            : "YYYY MMM Do",
    "syntaxTheme"           : "dark",
    "sort"                  : "boolean"
  },
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "encoding": "utf8",
    "recurse": true
  }
}
