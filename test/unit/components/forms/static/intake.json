{
  "name"          : "Sample Intake Form",
  "dataModelInit" : "loadPatient",
  "pageLayout"    : "paged",
  "validateOn"    : "change",
  "autosave"      : {
    "trigger" : "pageChange"
  },
  "pages"         : [
    {
      "name"   : "Page 1 - Embedded Forms and Collections",
      "fields" : [
        {
          "className"  : "sample-field-group",
          "fieldGroup" : [
            {
              "key"             : "sampleEmbeddedFullFormNoPage",
              "type"            : "embeddedForm",
              "templateOptions" : {
                "label" : "Embedded Full Form"
              },
              "data"            : {
                "embeddedType"  : "embeddableFullForm",
                "embeddedLabel" : "Embedded Form"
              }
            },
            {
              "key"             : "sampleEmbeddedFullFormPage1",
              "type"            : "embeddedForm",
              "templateOptions" : {
                "label" : "Embedded Full Form"
              },
              "data"            : {
                "embeddedType"  : "embeddableFullForm",
                "embeddedLabel" : "Embedded Form",
                "embeddedPage"  : 1
              }
            },
            {
              "key"             : "sampleEmbeddedFullFormLastPage",
              "type"            : "embeddedForm",
              "templateOptions" : {
                "label" : "Embedded Full Form"
              },
              "data"            : {
                "embeddedType"  : "embeddableFullForm",
                "embeddedLabel" : "Embedded Form",
                "embeddedPage"  : "Last Page"
              }
            }
          ]
        },
        {
          "className"  : "sample-field-group",
          "fieldGroup" : [
            {
              "key"             : "sampleEmbeddedSubform",
              "type"            : "embeddedFormCollection",
              "templateOptions" : {
                "label" : "Embedded Subform"
              },
              "data"            : {
                "embeddedType"  : "embeddableSubform",
                "embeddedLabel" : "Embedded Subform"
              }
            }
          ]
        },
        {
          "className"  : "sample-field-group",
          "fieldGroup" : [
            {
              "key"  : "sampleEmbeddedFormCollectionNoTypeTest",
              "type" : "embeddedFormCollection"
            }
          ]
        }
      ]
    },
    {
      "name"   : "Page 2 - Built-in Templates",
      "fields" : [
        {
          "key"  : "templates.instructions",
          "type" : "instructions"
        },
        {
          "key"  : "templates.input",
          "type" : "input"
        },
        {
          "key"  : "templates.checkbox",
          "type" : "checkbox"
        },
        {
          "key"  : "templates.multiCheckbox",
          "type" : "multiCheckbox"
        },
        {
          "key"  : "templates.radio",
          "type" : "radio"
        },
        {
          "key"  : "templates.select",
          "type" : "select"
        },
        {
          "key"  : "templates.textarea",
          "type" : "textarea"
        },
        {
          "key"  : "templates.dateComponents",
          "type" : "dateComponents"
        },
        {
          "key"  : "templates.dateComponentsPartial",
          "type" : "dateComponentsPartial"
        },
        {
          "key"  : "templates.typeahead",
          "type" : "typeahead"
        },
        {
          "key"  : "templates.typeaheadWithScript",
          "type" : "typeaheadWithScript"
        }
      ]
    },
    {
      "name"   : "Page 3 - Custom backend templates",
      "fields" : [
        {
          "key"             : "sampleCustomField",
          "type"            : "customTemplate",
          "templateOptions" : {
            "label" : "Here is a sample custom field"
          },
          "data"            : {
            "viewMode" : {
              "template" : "<span></span>"
            }
          }
        },
        {
          "key"             : "sampleCustomWrapper",
          "type"            : "customWrapperTemplate",
          "templateOptions" : {
            "label" : "Here is a sample custom field using a custom wrapper"
          },
          "data"            : {
            "viewMode" : {
              "templateUrl" : "testViewMode.html"
            }
          }
        }
      ]
    },
    {
      "name"   : "Page 4 - Validation",
      "fields" : [
        {
          "key"             : "RequiredInput",
          "type"            : "input",
          "templateOptions" : {
            "required" : true
          }
        },
        {
          "key"             : "RequiredSubform",
          "type"            : "embeddedFormCollection",
          "templateOptions" : {
            "required" : true
          },
          "data"            : {
            "embeddedType"  : "subformWithValidation",
            "embeddedLabel" : "Subform for Validation"
          }
        }
      ]
    },
    {
      "name"           : "Page 5 - Page Hide Expressions",
      "hideExpression" : "!model.RequiredInput",
      "fields"         : [
      ]
    }
  ]
}