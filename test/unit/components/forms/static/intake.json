{
  "name"         : "Sample Intake Form",
  "formType"     : "initial",
  "dataModelInit": "loadPatient",
  "pages"        : [
    {
      "name"  : "Page 1 - Embedded Forms and Collections",
      "fields": [
        {
          "className" : "sample-field-group",
          "fieldGroup": [
            {
              "key"            : "sampleEmbeddedFullForm",
              "type"           : "embeddedForm",
              "templateOptions": {
                "label": "Embedded Full Form"
              },
              "data"           : {
                "embeddedType" : "embeddableFullForm",
                "embeddedLabel": "Embedded Form"
              }
            },
            {
              "key"            : "sampleEmbeddedFullForm",
              "type"           : "embeddedForm",
              "templateOptions": {
                "label": "Embedded Full Form"
              },
              "data"           : {
                "embeddedType" : "embeddableFullForm",
                "embeddedLabel": "Embedded Form",
                "embeddedPage" : "Last Page"
              }
            }
          ]
        },
        {
          "className" : "sample-field-group",
          "fieldGroup": [
            {
              "key"            : "sampleEmbeddedSubform",
              "type"           : "embeddedFormCollection",
              "templateOptions": {
                "label": "Embedded Subform"
              },
              "data"           : {
                "embeddedType" : "embeddableSubform",
                "embeddedLabel": "Embedded Subform"
              }
            }
          ]
        }
      ]
    },
    {
      "name" : "Page 2 - Built-in Templates",
      "fields" : [
        {
          "key" : "templates.instructions",
          "type" : "instructions"
        },
        {
          "key" : "templates.input",
          "type" : "input"
        },
        {
          "key" : "templates.checkbox",
          "type" : "checkbox"
        },
        {
          "key" : "templates.multiCheckbox",
          "type" : "multiCheckbox"
        },
        {
          "key" : "templates.radio",
          "type" : "radio"
        },
        {
          "key" : "templates.select",
          "type" : "select"
        },
        {
          "key" : "templates.textarea",
          "type" : "textarea"
        },
        {
          "key" : "templates.dateComponents",
          "type" : "dateComponents"
        },
        {
          "key" : "templates.dateComponentsPartial",
          "type" : "dateComponentsPartial"
        },
        {
          "key" : "templates.typeahead",
          "type" : "typeahead"
        },
        {
          "key" : "templates.typeaheadWithScript",
          "type" : "typeaheadWithScript"
        }
      ]
    },
    {
      "name" : "Page 3 - Custom backend templates",
      "fields" : [
        {
          "key"            : "sampleCustomField",
          "type"           : "customTemplate",
          "templateOptions": {
            "label"   : "Here is a sample custom field",
            "required": true
          },
          "data" : {
            "viewMode" : {
              "template" : "<span></span>"
            }
          }
        },
        {
          "key"            : "sampleCustomWrapper",
          "type"           : "customWrapperTemplate",
          "templateOptions": {
            "label"   : "Here is a sample custom field using a custom wrapper"
          },
          "data" : {
            "viewMode" : {
              "templateUrl" : "viewMode.html"
            }
          }
        }
      ]
    }
  ]
}